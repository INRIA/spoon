@@
expression E;
expression LARGEST when matches "(WebSettings\\.TextSize\\.)?LARGEST";
expression LARGER when matches "(WebSettings\\.TextSize\\.)?LARGER";
expression NORMAL when matches "(WebSettings\\.TextSize\\.)?NORMAL";
expression SMALLER when matches "(WebSettings\\.TextSize\\.)?SMALLER";
expression SMALLEST when matches "(WebSettings\\.TextSize\\.)?SMALLEST";
@@
(
- E.setTextSize(LARGEST);
+ E.setTextZoom(200);
|
- E.setTextSize(LARGER);
+ E.setTextZoom(150);
|
- E.setTextSize(NORMAL);
+ E.setTextZoom(100);
|
- E.setTextSize(SMALLER);
+ E.setTextZoom(75);
|
- E.setTextSize(SMALLEST);
+ E.setTextZoom(50);
)
