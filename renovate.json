{
   "extends":[
      "config:base",
      "helpers:pinGitHubActionDigests"
   ],
   "ignoreDeps": ["org.apache.maven:maven-model"],
   "packageRules":[
      {
         "matchUpdateTypes":[
            "minor",
            "patch",
            "pin",
            "digest",
            "pinDigest",
            "lockFileMaintenance"
         ],
         "automerge":true
      }
   ],
   "nix": {
      "enabled": true
   },
   "lockFileMaintenance": {
      "enabled": true
   }
}
